<h1 class="mb-4">Jadwal Kuliah Semester Ini</h1>

<table class="table table-striped table-hover">
  <thead class="table-dark">
    <tr>
      <th>Hari</th>
      <th>Jam</th>
      <th>Mata Kuliah</th>
      <th>Dosen</th>
      <th>Ruang</th>
    </tr>
  </thead>
  <tbody>
    <% jadwal.forEach(j => { %>
    <tr>
      <td><%= j.hari %></td>
      <td><%= j.jam %></td>
      <td><%= j.matkul %></td>
      <td><%= j.dosen %></td>
      <td><%= j.ruang %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
